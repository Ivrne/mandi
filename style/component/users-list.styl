.users-list
  debullet()

  .avatar
    float left
    margin-right 20px
    avatar-size(95px)
    border-radius 3px

  .detail

    .username
      font-weight normal
      margin-bottom 5px

    .title
      ellipsis()
      font-style italic
      margin-bottom 5px
      font-size 16px

  li
    clearfix()
    vertical-rhythm()

  .meta
    font-size 13px